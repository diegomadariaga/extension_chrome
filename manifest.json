{
    "manifest_version": 3,
    "name": "selector html",
    "description": "selecciona elementos del html",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["storage", "activeTab"],
    "content_scripts": [
        {
            "matches": ["https://*/*", "http://*/*"],
            "js": ["content-script.js"]
        }
    ]
}
